{
    "seed_urls": [
        "https://www.agora.io/en/"
    ],
    "max_pages": 50,
    "max_workers": 3,
    "delay_between_requests": 1.0,
    "timeout": 10,
    "max_retries": 3,
    "user_agent": "WebCrawler/1.0",
    "respect_robots": true,
    "max_queue_size": 100,
    "stats_interval": 10,
    "output_dir": "crawled_data",
    "database": {
        "connection_string": "mongodb://localhost:27017/",
        "database_name": "web_crawler"
    }
}
